    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-require": "^1.0.1",
    "chai": "^3.5.0",
    "flow-typed": "^2.0.0-beta.8",
    "iflow-lodash": "^1.1.24",
    "mocha": "^3.0.2"
  }
